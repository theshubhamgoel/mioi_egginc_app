{
	"compilerOptions": {
		"lib": ["ES6", "DOM"],
		"outDir": "./dist/",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports" : true,
		"esModuleInterop" : true,
		"moduleResolution": "node",
		"resolveJsonModule": true
		}
  }